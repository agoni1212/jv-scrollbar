{
  "name": "jv-scrollbar",
  "description": "A Vue Component for Scrollbar",
  "version": "0.8.0",
  "author": "agoni1212 <agoni1212@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/jv-scrollbar.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agoni1212/jv-scrollbar.git"
  },
